{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2020": true
    },
    "extends": [
        "google"
    ],
    "parserOptions": {
        "ecmaVersion": 11
    },
    "rules": {
        "linebreak-style":"off",
        "indent":["error", 4],
        "comma-dangle": ["error", {
            "functions": "only-multiline"
        }]
    }
}
